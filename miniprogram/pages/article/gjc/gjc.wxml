<view style="width:100%;height:100%">
  <view class="page-body">
    <view class="page-section page-section-gap">
      <map id="myMap" style="width: 100%; height: 200px;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" covers="{{covers}}" scale="15" show-location="false" enable-scroll="false"></map>
    </view>
  </view>
  <view style="margin-top:45rpx;">
  <image bindtap='viewimg' src="cloud://test-mux64.7465-test-mux64-1259757584/校园生活/公交.png" mode="widthFix" style="width:100%"></image>
</view>
  <view class="c" style="padding:10rpx 20rpx;">
    <view>
      <view class="b" bindtap="moveToLocation">
        <image src="../../../images/icon/local1.png" class="s-img"/>   山西农业大学西门公交站（点击导航）
      </view>
      <view class="b" bindtap="moveToLocation1">
        <image src="../../../images/icon/local1.png" class="s-img"/>   山西农业大学新北门公交站（点击导航）
      </view>
    </view>
  </view>
</view>
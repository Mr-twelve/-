<view style="width:100%;height:100%">
  <view class="page-body">
    <view class="page-section page-section-gap">
      <map id="myMap" style="width: 100%; height: 200px;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" covers="{{covers}}" scale="15" show-location="false" enable-scroll="false"></map>
    </view>
  </view>
  <button bindtap="moveToLocation">点击导航到动车站/太谷西站</button>
  <image bindtap='viewimg' src="cloud://good-3mgiy.676f-good-3mgiy/校园生活/动车时刻表1.png" mode="widthFix" style="width:100%"></image>
  <image bindtap='viewimg2' src="cloud://good-3mgiy.676f-good-3mgiy/校园生活/动车时刻表2.png" mode="widthFix" style="width:100%"></image>
</view>